      >
        <van-radio name="{{true}}">是</van-radio>
        <van-radio name="{{false}}">否</van-radio>
      </van-radio-group>
    </van-cell> -->
    <van-cell required size="large" title="是否有高息私人贷">
      <van-radio-group
        slot="right-icon"
        value="{{ isInDebt }}"
        direction="horizontal"
        bind:change="indebtChange"
      >
        <van-radio name="{{true}}">是</van-radio>
        <van-radio name="{{false}}">否</van-radio>
      </van-radio-group>
    </van-cell>
    <van-field
      id="bankDebt"
      size="large"
      clearable
      value="{{ bankDebt }}"
      name="bankDebt"
      type="digit"
      label="银行总贷款额度"
      placeholder="请输入银行总贷款额度"
      error-message="{{bankDebtMessage}}"
      data-amounttype="bankDebt"
      bind:change="amountChange"
      title-width="7.2em"
    >
      <text slot="right-icon">万元</text>
    </van-field>
    <van-field
      id="creditCardDebt"
      size="large"
      clearable
      model:value="{{ creditCardDebt }}"
      name="creditCardDebt"
      type="digit"
      label="信用卡总额度"
      placeholder="请输入信用卡总额度"
      error-message="{{creditCardDebtMessage}}"
      data-amounttype="creditCardDebt"
      bind:change="amountChange"
    >
      <text slot="right-icon">万元</text>
    </van-field>
    <van-cell size="large" title="名下有无房产">
      <van-radio-group
        slot="right-icon"
        value="{{ hasHouse }}"
        direction="horizontal"
        bind:change="hasHouseChange"
      >
        <van-radio name="{{true}}">有</van-radio>
        <van-radio name="{{false}}">无</van-radio>
      </van-radio-group>
    </van-cell>
    <van-cell size="large" title="名下有无车产">
      <van-radio-group
        slot="right-icon"
        value="{{ hasCar }}"
        direction="horizontal"
        bind:change="hasCarChange"
      >
        <van-radio name="{{true}}">有</van-radio>
        <van-radio name="{{false}}">无</van-radio>
      </van-radio-group>
    </van-cell>
    <van-field
      id="reference"
      size="large"
      required
      clearable
      label="推荐人代码"
      placeholder="请输入推荐人代码"
      value="{{ reference }}"
      type="number"
      error-message="{{referenceMessage}}"
      bind:change="referenceChange"
    />
    <van-field
      id="applyDate"
      readonly
      required
      size="large"
      label="填表日期"
      right-icon="arrow"
      clickable
      value="{{applyDateTxt}}"
      placeholder="请选择填表日期"
      error-message="{{applyDateMessage}}"
      bind:tap="openApplyPicker"
    />
  </van-cell-group>
