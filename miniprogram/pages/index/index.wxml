<!--pages/index/index.wxml-->
<!-- 客户信息 -->
<van-toast id="van-toast" class="van-toast" />
<review-index wx:if="{{reviewApiLoaded && isReview}}"></review-index>
<view
  wx:if="{{reviewApiLoaded && !isReview}}"
  class="home-page"
  bindtap="checkUser"
>
  <van-cell-group id="userinfo" inset title="客户信息">
    <van-field
      size="large"
      required
      clearable
      value="{{ username }}"
      name="username"
      label="客户姓名"
      placeholder="(上传身份证可识别)"
      error-message="{{nameMessage}}"
      data-nametype="username"
      bind:change="nameChange"
    />
    <van-field
      id="phone"
      size="large"
      required
      clearable
      value="{{ phone }}"
      name="phone"
      type="number"
      label="手机号码"
      placeholder="请输入手机号码"
      error-message="{{phoneMessage}}"
      bind:change="phoneChange"
    />
    <van-field
      id="idcard"
      size="large"
      required
      clearable
      name="idcard"
      type="idcard"
      value="{{ idCard }}"
      label="身份证号"
      placeholder="(上传身份证可识别)"
      error-message="{{idCardMessage}}"
      bind:change="idCardChange"
    />
    <!-- <van-field
      id="bornDate"
      readonly
      required
      size="large"
      label="出生日期"
      right-icon="arrow"
      clickable
      value="{{bornDateTxt}}"
      placeholder="(上传身份证可识别)"
      error-message="{{bornDateMessage}}"
      bind:tap="openBornPicker"
    /> -->
    <van-field
      id="areaList"
      readonly
      required
      size="large"
      label="现住家庭地址"
      right-icon="arrow"
      clickable
      value="{{areaTxt}}"
      placeholder="请选择省市区"
      error-message="{{areaMessage}}"
      data-area-show-type="address"
      bind:tap="openAreaPicker"
    />
    <van-field
      id="address"
      required
      size="large"
      value="{{ address }}"
      name="address"
      label="详细地址"
      type="textarea"
      placeholder="请输入详细地址"
      data-nametype="address"
      bind:change="textChange"
      error-message="{{addressMessage}}"
      autosize
    />
    <van-field
      id="marryStatus"
      readonly
      required
      size="large"
      label="婚姻状况"
      value="{{marryStatusTxt}}"
      placeholder="请选择婚姻状况"
      error-message="{{marryStatusMessage}}"
      bind:tap="openMarryPicker"
      right-icon="arrow"
      clickable
    />
    <van-field
      wx:if="{{marryStatus===0}}"
      id="marryname"
      size="large"
      required
      clearable
      value="{{ marryname }}"
      name="marryname"
      label="配偶姓名"
      placeholder="请输入配偶姓名"
      error-message="{{marrynameMessage}}"
      data-nametype="marryname"
      bind:change="nameChange"
    />
  </van-cell-group>
  <!-- 身份证正反面 -->
  <van-cell-group id="idcardCamera" inset title="上传身份证" border="{{false}}">
  </van-cell-group>
  <view class="idcard-upload-wrap">
    <van-uploader
      accept="image"
      sizeType="{{['compressed']}}"
      max-count="1"
      deletable="{{ true }}"
      file-list="{{ idCardFront }}"
      bind:after-read="afterIdCardFrontRead"
      show-upload="{{false}}"
      preview-size="210rpx"
      image-fit="aspectFit"
      bind:delete="deleteIdCardFront"
    >
      <view class="idcard-upload">
        <image class="idcard-upload-img" src="../../images/idcard_add.png" />
        <view class="idcard-desc">身份证人像面</view>
      </view>
    </van-uploader>
    <van-uploader
      accept="image"
      sizeType="{{['compressed']}}"
      max-count="1"
      deletable="{{ true }}"
      file-list="{{ idCardBack }}"
      bind:after-read="afterIdCardBackRead"
      show-upload="{{false}}"
      preview-size="210rpx"
      image-fit="aspectFit"
      bind:delete="deleteIdCardBack"
    >
      <view class="idcard-upload">
        <image class="idcard-upload-img" src="../../images/idcard_add.png" />
        <view class="idcard-desc">身份证国徽面</view>
      </view>
    </van-uploader>
  </view>
  <!-- 申请资料 -->
  <van-cell-group inset title="申请资料">
    <van-field
      id="loanAmount"
      size="large"
      clearable
      value="{{ loanAmount }}"
      name="loanAmount"
      type="digit"
      label="申请额度"
      placeholder="请输入申请额度"
      error-message="{{loanAmountMessage}}"
      data-amounttype="loanAmount"
      bind:change="amountChange"
    >
      <text slot="right-icon">万元</text>
    </van-field>
    <van-field
      id="useWay"
      readonly
      required
      size="large"
      label="借款用途"
      right-icon="arrow"
      clickable
      value="{{useWayTxt}}"
      placeholder="请选择借款用途"
      error-message="{{useWayMessage}}"
      bind:tap="openUseWayPicker"
    />
    <van-field
      size="large"
      model:value="{{ useWayMark }}"
      label="备注"
      type="textarea"
      placeholder="请输入备注（借款用途）"
      autosize
    />
    <van-cell required size="large">
      <view slot="title">
        <view class="van-cell-text">家人是否支持贷款</view>
      </view>
      <van-radio-group
        slot="right-icon"
        value="{{ isFamilySupport }}"
        direction="horizontal"
        bind:change="familySupportChange"
      >
        <van-radio name="{{true}}">是</van-radio>
        <van-radio name="{{false}}">否</van-radio>
      </van-radio-group>
    </van-cell>
    <van-field
      id="companyName"
      size="large"
      required
      clearable
      value="{{ companyName }}"
      name="companyName"
      data-nametype="companyName"
      label="公司全称"
      placeholder="请输入公司全称"
      error-message="{{companyNameMessage}}"
      bind:change="textChange"
    />
    <van-field
      id="companyMaster"
      size="large"
      required
      clearable
      value="{{ companyMaster }}"
      name="companyMaster"
      data-nametype="companyMaster"
      label="公司法人"
      placeholder="请输入公司法人"
      error-message="{{companyMasterMessage}}"
      bind:change="nameChange"
    />
  </van-cell-group>
  <!-- 营业执照 -->
  <van-cell-group id="bizLicense" inset title="上传营业执照" border="{{false}}">
    <view class="idcard-upload-wrap">
      <van-uploader
        accept="image"
        sizeType="{{['compressed']}}"
        max-count="1"
        deletable="{{ true }}"
        file-list="{{ bizLicense }}"
        bind:after-read="afterBizLicenseRead"
        show-upload="{{false}}"
        preview-size="300rpx"
        image-fit="aspectFit"
        bind:delete="deleteBizLicense"
      >
        <view class="idcard-upload">
          <image class="idcard-upload-img" src="../../images/idcard_add.png" />
          <view class="idcard-desc">营业执照</view>
        </view>
      </van-uploader>
    </view>
  </van-cell-group>
  <van-cell-group inset border="{{false}}">
    <van-cell required size="large" title="经营年限">
      <van-stepper
        name="operYears"
        slot="right-icon"
        min="{{0}}"
        step="{{0.5}}"
        decimal-length="{{1}}"
        input-width="100rpx"
        value="{{ operYears }}"
        bind:change="operYearsChange"
      />
    </van-cell>
    <van-cell size="large" title="员工人数">
      <van-stepper
        name="companyMember"
        slot="right-icon"
        min="{{0}}"
        step="{{10}}"
        integer
        input-width="150rpx"
        value="{{ companyMember }}"
        bind:change="companyMemberChange"
      />
    </van-cell>
    <!-- <van-field
      id="salary"
      size="large"
      clearable
      value="{{ salary }}"
      name="salary"
      type="digit"
      title-width="7.2em"
      label="员工工资（均）"
      placeholder="请输入员工工资（均）"
      error-message="{{salaryMessage}}"
      data-amounttype="salary"
      bind:change="amountChange"
    >
      <text slot="right-icon">万元</text>
    </van-field> -->
    <van-field
      id="flowingWater"
      size="large"
      clearable
      value="{{ flowingWater }}"
      name="flowingWater"
      type="digit"
      label="月收入"
      placeholder="请输入月收入"
      error-message="{{flowingWaterMessage}}"
      data-amounttype="flowingWater"
      bind:change="amountChange"
    >
      <text slot="right-icon">万元</text>
    </van-field>
    <van-field
      id="annualTurnover"
      size="large"
      clearable
      value="{{ annualTurnover }}"
      name="annualTurnover"
      type="digit"
      label="年营业额"
      placeholder="请输入年营业额"
      error-message="{{annualTurnoverMessage}}"
      data-amounttype="annualTurnover"
      bind:change="amountChange"
    >
      <text slot="right-icon">万元</text>
    </van-field>
    <van-cell required size="large" title="订单是否充足">
      <van-radio-group
        slot="right-icon"
        value="{{ isSufficient }}"
        direction="horizontal"
        bind:change="sufficientChange"
      >
        <van-radio name="{{true}}">是</van-radio>
        <van-radio name="{{false}}">否</van-radio>
      </van-radio-group>
    </van-cell>
    <van-field
      id="equipmentPrice"
      size="large"
      clearable
      value="{{ equipmentPrice }}"
      name="equipmentPrice"
      type="digit"
      label="设备价值"
      placeholder="请输入设备价值"
      error-message="{{equipmentPriceMessage}}"
      data-amounttype="equipmentPrice"
      bind:change="amountChange"
    >
      <text slot="right-icon">万元</text>
    </van-field>
    <van-cell size="large" title="设备是否在押">
      <van-radio-group
        slot="right-icon"
        value="{{ isEquipmentDetain }}"
        direction="horizontal"
        bind:change="equipmentDetainChange"
      >
        <van-radio name="{{true}}">是</van-radio>
        <van-radio name="{{false}}">否</van-radio>
      </van-radio-group>
    </van-cell>
    <van-field
      id="businessScale"
      readonly
      required
      size="large"
      label="企业规模"
      right-icon="arrow"
      clickable
      value="{{businessScaleTxt}}"
      placeholder="请选择企业规模"
      error-message="{{businessScaleMessage}}"
      bind:tap="openbusinessScalePicker"
    />
    <van-cell size="large" title="占股百分比">
      <view class="flex" slot="right-icon">
        <van-stepper
          name="sharePercent"
          min="{{0}}"
          max="{{100}}"
          step="{{1}}"
          decimal-length="{{2}}"
          input-width="150rpx"
          value="{{ sharePercent }}"
          bind:change="sharePercentChange"
        />
        <text>%</text>
      </view>
    </van-cell>
    <van-field
      id="siteArea"
      size="large"
      clearable
      value="{{ siteArea }}"
      name="siteArea"
      type="digit"
      label="场地面积"
      placeholder="请输入场地面积"
      error-message="{{siteAreaMessage}}"
      data-amounttype="siteArea"
      bind:change="amountChange"
    >
      <text slot="right-icon">㎡</text>
    </van-field>
    <van-field
      id="monthlyRent"
      size="large"
      clearable
      value="{{ monthlyRent }}"
      error-message="{{monthlyRentMessage}}"
      name="monthlyRent"
      type="digit"
      label="场地月缴租金"
      placeholder="请输入场地月缴租金"
      bind:change="monthlyRentChange"
    >
      <text slot="right-icon">万元</text>
    </van-field>
    <!-- <van-field
      id="annualIncome"
      size="large"
      clearable
